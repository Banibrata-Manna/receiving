{
  "name": "Receiving",
  "version": "0.0.1",
  "author": "Hotwax Commerce",
  "homepage": "http://hotwax.co",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package-linux": "electron-packager . Receiving --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "create-debian-installer": "electron-installer-debian --src release-builds/Receiving-linux-x64/ --arch amd64 --config debian.json",
    "package-mac": "electron-packager . Receiving --overwrite --platform=darwin --arch=x64 --icon=src/assets/icon/mac/Receiving.icns --prune=true --out=release-builds",
    "create-mac-installer": "electron-installer-dmg ./release-builds/Receiving-darwin-x64/Receiving.app Receiving --out=release-builds --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.1.6",
